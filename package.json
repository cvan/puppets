{
  "name": "vps-vr",
  "version": "0.0.1",
  "description": "Virtual Puppet Studio",
  "scripts": {
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "gulp dev",
    "watch": "gulp watch",
    "clean": "gulp clean",
    "build": "gulp dist",
    "prod": "npm run clean && npm run build && npm run server",
    "prod:server": "node ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/datavized/puppets.git"
  },
  "homepage": "https://github.com/datavized/puppets",
  "bugs": {
    "url": "https://github.com/datavized/puppets/issues"
  },
  "author": "Brian Chirls <brian@datavized.com> (http://datavized.com)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "electricity": "^1.7.0",
    "node-static": "^0.7.9",
    "static": "^2.0.0"
  },
  "jshintConfig": {
    "globals": {
      "console": true
    },
    "bitwise": true,
    "browser": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "esversion": 6,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "strict": "global",
    "immed": true,
    "iterator": true,
    "latedef": true,
    "maxparams": 5,
    "newcap": true,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": true,
    "quotmark": "single",
    "shadow": true,
    "singleGroups": true,
    "undef": true,
    "unused": true
  }
}
